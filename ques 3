#include <iostream>
using namespace std;
struct Point {
    float x, y;
};
int main() {
   Point points[7];
    int firstQuadrantCount = 0;

    for (int i = 0; i < 7; i++) {
        cout << "Enter coordinates for point " << i + 1 << " (x y): ";
        cin >> points[i].x >> points[i].y;
    }
 for (int i = 0; i < 7; i++) {
        if (points[i].x > 0 && points[i].y > 0) {
            firstQuadrantCount++;
        }
    }
 cout << "Number of points in the first quadrant: " << firstQuadrantCount << endl;
return 0;
}

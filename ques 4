#include <iostream>
#include<string>
#include<algorithm>
using namespace std;
struct SalesRecord{
    string month;
    float amount;
};
    
int main() {
   SalesRecord structures[12];
   for(int i=0 ; i<12 ; i++){
       cin>>structures[i].month;
       cin>>structures[i].amount;
   }
   float maxAmount=0;
   float minAmount=10000000;
   for(int i=0 ; i<12 ; i++){
    maxAmount=max(maxAmount,structures[i].amount);
    minAmount=min(minAmount,structures[i].amount);
   }
   for(int i=0 ;i<12 ; i++){
    if(structures[i].amount==maxAmount)cout<<"month with maximum sales : "<<structures[i].month<<endl;
   }
     for(int i=0 ;i<12 ; i++){
    if(structures[i].amount==minAmount)cout<<"month with minimum sales : "<<structures[i].month<<endl;
   }
    return 0;
}
